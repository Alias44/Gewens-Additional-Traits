<?xml version="1.0" encoding="UTF-8"?>
<Defs>
	<!-- Heroic Traits -->
	<TraitDef ParentName="GAT_HeroicBase">
		<defName>GAT_Hercules</defName>
		<degreeDatas>
			<li>
				<label>Herculean</label>
				<description>Brave {PAWN_nameDef}, born with impossible strength and force of will.</description>
				<statOffsets>
					<MentalBreakThreshold>-1</MentalBreakThreshold>
					<CarryingCapacity>150</CarryingCapacity>
					<ArmorRating_Blunt>0.5</ArmorRating_Blunt>
					<ArmorRating_Sharp>0.7</ArmorRating_Sharp>
				</statOffsets>
				<skillGains>
					<li>
						<key>Melee</key>
						<value>6</value>
					</li>
				</skillGains>
			</li>
		</degreeDatas>
		<requiredWorkTags>
			<li>Violent</li>
		</requiredWorkTags>
	</TraitDef>

	<TraitDef ParentName="GAT_HeroicBase">
		<defName>GAT_Achilles</defName>
		<degreeDatas>
			<li>
				<label>Achillean</label>
				<description>{PAWN_nameDef} is said to have been dipped in the river of death, {PAWN_pronoun} seems invulnerable in battle.</description>
				<statOffsets>
					<ImmunityGainSpeed>1</ImmunityGainSpeed>
					<MoveSpeed>.5</MoveSpeed>
					<ArmorRating_Blunt>0.8</ArmorRating_Blunt>
					<ArmorRating_Sharp>0.8</ArmorRating_Sharp>
				</statOffsets>
				<skillGains>
					<li>
						<key>Melee</key>
						<value>2</value>
					</li>
					<li>
						<key>Shooting</key>
						<value>2</value>
					</li>
				</skillGains>
			</li>
		</degreeDatas>
		<requiredWorkTags>
			<li>Violent</li>
		</requiredWorkTags>
	</TraitDef>

	<TraitDef ParentName="GAT_HeroicBase">
		<defName>GAT_Odysseus</defName>
		<degreeDatas>
			<li>
				<label>Odyssean</label>
				<description>Craftier by far than any mortal man, {PAWN_nameDef} is also a very capable warrior.</description>
				<statOffsets>
					<GlobalLearningFactor>1</GlobalLearningFactor>
					<ShootingAccuracyPawn>.5</ShootingAccuracyPawn>
					<MeleeHitChance>.5</MeleeHitChance>
				</statOffsets>
				<skillGains>
					<li>
						<key>Melee</key>
						<value>1</value>
					</li>
					<li>
						<key>Shooting</key>
						<value>1</value>
					</li>
				</skillGains>
			</li>
		</degreeDatas>
		<requiredWorkTags>
			<li>Violent</li>
		</requiredWorkTags>
	</TraitDef>

	<!-- God Traits -->
	<TraitDef ParentName="GAT_DivineBase">
		<defName>GAT_Ares</defName>
		<degreeDatas>
			<li>
				<label>Ares' Wrath</label>
				<description>Feared by {PAWN_objective} enemies and worshiped by warriors, {PAWN_nameDef} is a bloodthirsty embodiment of war.</description>
				<statOffsets>
					<ShootingAccuracyPawn>1</ShootingAccuracyPawn>
					<MoveSpeed>.5</MoveSpeed>
					<MeleeHitChance>1</MeleeHitChance>
					<ArmorRating_Blunt>1</ArmorRating_Blunt>
					<ArmorRating_Sharp>1.2</ArmorRating_Sharp>
				</statOffsets>
				<skillGains>
					<li>
						<key>Melee</key>
						<value>8</value>
					</li>
				</skillGains>
			</li>
		</degreeDatas>
		<requiredWorkTags>
			<li>Violent</li>
		</requiredWorkTags>
	</TraitDef>

	<TraitDef ParentName="GAT_DivineBase">
		<defName>GAT_Artemis</defName>
		<commonality>0</commonality>
		<commonalityFemale>0.05</commonalityFemale>
		<degreeDatas>
			<li>
				<label>Artemis' Blessing</label>
				<description>{PAWN_nameDef}'s natural instinct to protect and nurture the people led {PAWN_objective} to champion the hunt.</description>
				<statOffsets>
					<HuntingStealth>0.5</HuntingStealth>
					<ShootingAccuracyPawn>1</ShootingAccuracyPawn>
					<AimingDelayFactor>-1</AimingDelayFactor>
					<ButcheryFleshSpeed>1</ButcheryFleshSpeed>
					<CookSpeed>1</CookSpeed>
					<ArmorRating_Blunt>1</ArmorRating_Blunt>
					<ArmorRating_Sharp>1.2</ArmorRating_Sharp>
					<ForagedNutritionPerDay>1</ForagedNutritionPerDay>
				</statOffsets>
				<skillGains>
					<li>
						<key>Shooting</key>
						<value>8</value>
					</li>
				</skillGains>
			</li>
		</degreeDatas>
		<requiredWorkTags>
			<li>Violent</li>
		</requiredWorkTags>
	</TraitDef>

	<TraitDef ParentName="GAT_DivineBase">
		<defName>GAT_Apollo</defName>
		<degreeDatas>
			<li>
				<label>Apollo's Harmony</label>
				<description>Patron of the arts and student of the bow, {PAWN_nameDef} will bring beauty to {PAWN_possessive} world, one way or another.</description>
				<statOffsets>
					<SculptingSpeed MayRequire="Alias.DegeneralizeWork">1</SculptingSpeed>
					<ShootingAccuracyPawn>1</ShootingAccuracyPawn>
					<AimingDelayFactor>-1</AimingDelayFactor>
					<ArmorRating_Blunt>1</ArmorRating_Blunt>
					<ArmorRating_Sharp>1.2</ArmorRating_Sharp>
				</statOffsets>
				<skillGains>
					<li>
						<key>Shooting</key>
						<value>4</value>
					</li>
					<li>
						<key>Artistic</key>
						<value>4</value>
					</li>
				</skillGains>
				<allowedMeditationFocusTypes>
					<li>Artistic</li>
				</allowedMeditationFocusTypes>
			</li>
		</degreeDatas>
		<requiredWorkTags>
			<li>Violent</li>
			<li>Artistic</li>
		</requiredWorkTags>
	</TraitDef>

	<TraitDef ParentName="GAT_DivineBase">
		<defName>GAT_Hades</defName>
		<degreeDatas>
			<li>
				<label>Hades' Wealth</label>
				<description>{PAWN_nameDef} knows where the riches are buried, {PAWN_pronoun} also knows that the dead can't take it with them.</description>
				<statOffsets>
					<ArmorRating_Blunt>1</ArmorRating_Blunt>
					<ArmorRating_Sharp>1.2</ArmorRating_Sharp>
				</statOffsets>
				<skillGains>
					<li>
						<key>Mining</key>
						<value>8</value>
					</li>
				</skillGains>
				<allowedMeditationFocusTypes>
					<li>Morbid</li>
				</allowedMeditationFocusTypes>
			</li>
		</degreeDatas>
		<requiredWorkTags>
			<li>Mining</li>
		</requiredWorkTags>
	</TraitDef>

	<TraitDef ParentName="GAT_DivineBase">
		<defName>GAT_Aphrodite</defName>
		<degreeDatas>
			<li>
				<label>Aphrodite's Grace</label>
				<description>So wondrous is {PAWN_nameDef} to behold that one look can inspire tears, desire, jealousy, and love.</description>
				<statOffsets>
					<NegotiationAbility>1</NegotiationAbility>
					<SocialImpact>1</SocialImpact>
					<TradePriceImprovement>1</TradePriceImprovement>
					<ArmorRating_Blunt>1</ArmorRating_Blunt>
					<ArmorRating_Sharp>1.2</ArmorRating_Sharp>
				</statOffsets>
				<skillGains>
					<li>
						<key>Social</key>
						<value>8</value>
					</li>
				</skillGains>
			</li>
		</degreeDatas>
		<requiredWorkTags>
			<li>Social</li>
		</requiredWorkTags>
	</TraitDef>

	<TraitDef ParentName="GAT_DivineBase">
		<defName>GAT_Demeter</defName>
		<degreeDatas>
			<li>
				<label>Demeter's Bounty</label>
				<description>{PAWN_nameDef} has a special connection to the roots. Plants grow, flower, and die at {PAWN_possessive} order.</description>
				<statOffsets>
					<PlantWorkSpeed>1</PlantWorkSpeed>
					<PlantHarvestYield>2</PlantHarvestYield>
					<!--<HarvestFailChance>-1</HarvestFailChance>-->
					<ForagedNutritionPerDay>1</ForagedNutritionPerDay>
				</statOffsets>
				<skillGains>
					<li>
						<key>Plants</key>
						<value>8</value>
					</li>
				</skillGains>
				<allowedMeditationFocusTypes>
					<li>Natural</li>
				</allowedMeditationFocusTypes>
			</li>
		</degreeDatas>
	</TraitDef>

	<TraitDef ParentName="GAT_DivineBase">
		<defName>GAT_Pan</defName>
		<degreeDatas>
			<li>
				<label>Pan's Blessing</label>
				<description>Companion to creatures both mundane and fantastic, {PAWN_nameDef} protects and nurtures {PAWN_possessive} flock.</description>
				<statOffsets>
					<TameAnimalChance>1</TameAnimalChance>
					<TrainAnimalChance>1</TrainAnimalChance>
					<ArmorRating_Blunt>1</ArmorRating_Blunt>
					<ArmorRating_Sharp>1.2</ArmorRating_Sharp>
				</statOffsets>
				<skillGains>
					<li>
						<key>Animals</key>
						<value>8</value>
					</li>
				</skillGains>
			</li>
		</degreeDatas>
		<requiredWorkTags>
			<li>Artistic</li>
			<li>PlantWork</li>
		</requiredWorkTags>
	</TraitDef>

	<TraitDef ParentName="GAT_DivineBase">
		<defName>GAT_Hephaestus</defName>
		<degreeDatas>
			<li>
				<label>Hephaestus' Forge</label>
				<description>{PAWN_nameDef} crafts with fire and forge as an artist would with paint and brush.</description>
				<statOffsets>
					<ConstructSuccessChance>2</ConstructSuccessChance>
					<!--<ConstructFailChance>-1</ConstructFailChance>-->
					<ConstructionSpeed>1</ConstructionSpeed>
					<SmithingSpeed MayRequire="Alias.DegeneralizeWork">1</SmithingSpeed>
					<SmeltingSpeed>1</SmeltingSpeed>
					<GeneralLaborSpeed>.5</GeneralLaborSpeed>
					<!-- removed  via patch if DegeneralizeWork is loaded-->
					<ArmorRating_Blunt>1</ArmorRating_Blunt>
					<ArmorRating_Sharp>1.2</ArmorRating_Sharp>
				</statOffsets>
				<skillGains>
					<li>
						<key>Crafting</key>
						<value>4</value>
					</li>
					<li>
						<key>Construction</key>
						<value>4</value>
					</li>
				</skillGains>
			</li>
		</degreeDatas>
		<requiredWorkTags>
			<li>Crafting</li>
			<li>ManualSkilled</li>
		</requiredWorkTags>
	</TraitDef>
	
	<TraitDef ParentName="GAT_DivineBase">
		<defName>GAT_Asclepius</defName>
		<degreeDatas>
			<li>
				<label>Asclepius' Healing</label>
				<description>{PAWN_nameDef}'s healing can cure the sick and keep death itself at bay.</description>
				<statOffsets>
					<MedicalTendSpeed>1</MedicalTendSpeed>
					<MedicalOperationSpeed>1</MedicalOperationSpeed>
					<MedicalSurgerySuccessChance>1</MedicalSurgerySuccessChance>
					<MedicalTendQuality>1</MedicalTendQuality>
					<ArmorRating_Blunt>1</ArmorRating_Blunt>
					<ArmorRating_Sharp>1.2</ArmorRating_Sharp>
				</statOffsets>
				<skillGains>
					<li>
						<key>Medicine</key>
						<value>8</value>
					</li>
				</skillGains>
			</li>
		</degreeDatas>
		<requiredWorkTags>
			<li>Caring</li>
		</requiredWorkTags>
	</TraitDef>

	<TraitDef ParentName="GAT_DivineBase">
		<defName>GAT_Athena</defName>
		<degreeDatas>
			<li>
				<label>Athena's Wisdom</label>
				<description>{PAWN_nameDef} commands the wisdom of the ages. {PAWN_pronoun} is a master tactician and inventor extraordinaire with few intellectual rivals.</description>
				<statOffsets>
					<GlobalLearningFactor>1</GlobalLearningFactor>
					<ResearchSpeed>1</ResearchSpeed>
					<ArmorRating_Blunt>1</ArmorRating_Blunt>
					<ArmorRating_Sharp>1.2</ArmorRating_Sharp>
				</statOffsets>
				<skillGains>
					<li>
						<key>Intellectual</key>
						<value>8</value>
					</li>
				</skillGains>
			</li>
		</degreeDatas>
		<requiredWorkTags>
			<li>ManualSkilled</li>
			<li>ManualSkilled</li>
		</requiredWorkTags>
	</TraitDef>

	<TraitDef ParentName="GAT_DivineBase">
		<defName>GAT_Hermes</defName>
		<degreeDatas>
			<li>
				<label>Hermes' Haste</label>
				<!--description>It is said that {PAWN_nameDef} can leap the space between lightning and thunder, outfox the fox king, and reach tomorrow before the sun.</description-->
				<description>{PAWN_nameDef} is quick to close a deal and even quicker to close a distance</description>
				<statOffsets>
					<EatingSpeed>1</EatingSpeed>
					<WorkSpeedGlobal>0.5</WorkSpeedGlobal>
					<MoveSpeed>1</MoveSpeed>
					<ArmorRating_Blunt>1</ArmorRating_Blunt>
					<ArmorRating_Sharp>1.2</ArmorRating_Sharp>
				</statOffsets>
				<skillGains>
					<li>
						<key>Intellectual</key>
						<value>8</value>
					</li>
				</skillGains>
			</li>
		</degreeDatas>
	</TraitDef>
</Defs>